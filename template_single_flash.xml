<?xml version="1.0" encoding="UTF-8"?>
<Module>
    <ModulePrefs title="NBA Playoffs Gadget"
    directory_title="NBA Playoffs Gadget"
    title_url="http://nba.com/unbelievable/"
    description="Latest unbelievable scores, stats, highlights, and schedules for the 2007 NBA Playoffs"
    screenshot="http://nba.goodbysilverstein.com/google_gadget/google_config/screenshot.jpg"
    thumbnail="http://nba.goodbysilverstein.com/google_gadget/google_config/thumbnail.jpg"
    author="NBA"
    author_link="http://nba.com/unbelievable/"
    width="300"
    height="250">
    <Require feature="flash"/>
  </ModulePrefs>
  <UserPref name="clickurl" datatype="hidden" default_value="http://gadgetads.googlecode.com/svn/trunk/test_clickurl.html?url="/>
  <UserPref name="flashurl" datatype="hidden" default_value=""/>
  <UserPref name="desturl" datatype="hidden" default_value=""/>
  <Content type="html"><![CDATA[
    <div id="fc"></div>
    <script>
      var prefs = new _IG_Prefs();
      var clickUrl = prefs.getString('clickurl');
      var flashUrl = prefs.getString('flashurl');
      var destUrl = clickUrl + _esc(prefs.getString('desturl'));
      
      var fv = 'clickTAG=' + _esc(destUrl);
      _IG_EmbedFlash(_IG_GetCachedUrl(flashUrl), 'fc', {
          width: 300,
          height: 250,
          flashvars: fv
        });
    </script>
    
    
