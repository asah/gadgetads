<?xml version="1.0" encoding="UTF-8" ?>
<Module>
<ModulePrefs title="My Gadget Ad" width="300" height="250" scrolling="false">
<Require feature="ads"/>
<Require feature="flash"/>
</ModulePrefs>
<UserPref name="clickurl" datatype="hidden" default_value="DEBUG"/>
<UserPref name="aiturl" datatype="hidden" default_value="DEBUG"/>
<Content type="html"><![CDATA[
<!--
Embeds a single Flash file in the gadget ad and sets the dimensions
to match the ad size.
-->
<div id="fc"></div>

<script>
	function clickThrough() {  
        ADS_ClickDestinationUrl('http://www.mobinil.com/personal/services/calltones/');
    }       
var swfUrl = 'http://activedd.googlecode.com/svn/trunk/Google_Ads/mobinil/mobinilcalltonefinal/as2/300_250/Arabic/adArAs2.swf';
var fWidth = 300;
var fHeight = 250;
_IG_EmbedCachedFlash(swfUrl, 'fc', {
swf_version: 8,
width: fWidth,
height: fHeight
});
</script>
]]></Content>
</Module>
