<?xml version="1.0" encoding="UTF-8"?>
<Module>
  <ModulePrefs title="My Gadget Ad" width="300" height="250" scrolling="false">
    <Require feature="ads"/>
  </ModulePrefs>
  <UserPref name="clickurl" datatype="hidden" default_value="DEBUG"/>
  <UserPref name="aiturl" datatype="hidden" default_value="DEBUG"/>
  <Content type="html"><![CDATA[
    <div>clickurl = [__UP_clickurl__]</div>
    <div>aiturl = [__UP_aiturl__]</div>
    
    <script>
      document.write('<br><br>from js...');
      document.write('clickurl = __UP_clickurl__');
      document.write('<br/>');
      document.write('aiturl = __UP_aiturl__');
    </script>
  ]]></Content>
</Module>