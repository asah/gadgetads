<?xml version="1.0" encoding="UTF-8" ?>
<Module>
  <ModulePrefs title="Sun BB and Q TEST" width="300" height="250">
    <Require feature="flash" />
  </ModulePrefs>
  <UserPref name="clickurl" datatype="hidden" default_value="http://gadgetads.googlecode.com/svn/trunk/test_clickurl.html?url="/>  
  <Content type="html"><![CDATA[
    <div id="flashContainer"></div>
    <script>
      var prefs = new _IG_Prefs();
      var clickUrl = prefs.getString('clickurl');
      
      _IG_EmbedFlash('http://gadgetads.googlecode.com/svn/trunk/sunbbq/bb_widget.swf', 'flashContainer', {
          width: 300,
          height: 250,
          wmode: 'transparent',
          movie: 'http://gadgetads.googlecode.com/svn/trunk/sunbbq/bb_widget.swf',
          flashVars: 'clickUrl=' + clickUrl
        });
    </script>
  ]]></Content>
</Module>