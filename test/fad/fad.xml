<?xml version="1.0" encoding="UTF-8" ?>
<Module>
  <ModulePrefs title="My Flash Gadget Ad"
    width="300"
    height="250"
    thumbnail=""
    scrolling="false">
    <Require feature="ads"/>
    <Require feature="flash"/>
  </ModulePrefs>
  <UserPref name="clickurl" datatype="hidden" default_value="DEBUG"/>
  <UserPref name="aiturl" datatype="hidden" default_value="DEBUG"/>
  <Content type="html"><![CDATA[
    <div id="container"></div>
    <script>
      _IG_EmbedFlash("http://www.corp.google.com/~wangz/fad/fad.swf", "container",
        {width: 300, height: 250});
    </script>
  ]]></Content>
</Module>
