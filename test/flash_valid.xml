<?xml version="1.0" encoding="UTF-8" ?>
<!--
  Gadget ad should pass all validation requirements
-->
<Module>
  <ModulePrefs title="My Flash Gadget Ad"
    width="300"
    height="250"
    thumbnail=""
    scrolling="false">
    <Require feature="ads"/>
    <Require feature="flash"/>
  </ModulePrefs>
  <UserPref name="clickurl" datatype="hidden" default_value="DEBUG"/>
  <UserPref name="aiturl" datatype="hidden" default_value="DEBUG"/>
  <Content type="html"><![CDATA[
    <div id="container"></div>
    <script>
      var swfUrl = 'http://gadgetads.googlecode.com/svn/trunk/test/test_content/fad.swf';
      _IG_EmbedCachedFlash(swfUrl, 'container', {
        width: 300, 
        height: 250
      });
    </script>
  ]]></Content>
</Module>
