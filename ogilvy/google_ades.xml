<?xml version="1.0" encoding="UTF-8"?>
<Module>
  <ModulePrefs title="Ades"
      title_url=""
      description=""
      author=""
      author_email=""
      author_affiliation=""
      author_location=""
      width="728"
      height="90"
      screenshot=""
      thumbnail=""
      scaling="false"
      scrolling="false">
    <Require feature="interaction-tracking"/>
	</ModulePrefs>
	<UserPref name="clickurl" datatype="hidden" default_value="http://gadgetads.googlecode.com/svn/trunk/test_clickurl.html?adurl="/>
	<UserPref name="aiturl" datatype="hidden"/>
	<Content type="html"><![CDATA[
		<div id="container"></div>
		<script>
			var prefs = new _IG_Prefs();
			var redirectUrl = prefs.getString('clickurl') + _esc('http://www.ades.com.br');
			_IG_EmbedFlash(_IG_GetCachedUrl('http://www.ades.com.br/campanhas/superbanner.swf'), 'container', {
			swf_version: 6,
			id: "flashid",
			width: 728,
			height: 90,
			flashvars: 'url=' + _esc(redirectUrl)
			});
		});
		</script>
	]]></Content>
</Module>